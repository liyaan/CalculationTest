<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gallery_navigation"
    app:startDestination="@id/galleryFragment">

    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.liyaan.calculationtest.gallery.fragment.GalleryFragment"
        android:label="Gallery"
        tools:layout="@layout/fragment_gallery">
        <action
            android:id="@+id/action_galleryFragment_to_photoFragment"
            app:destination="@id/photoFragment" />
        <action
            android:id="@+id/action_galleryFragment_to_pagerPhotoFragment"
            app:destination="@id/pagerPhotoFragment" />
    </fragment>
    <fragment
        android:id="@+id/photoFragment"
        android:name="com.liyaan.calculationtest.gallery.fragment.PhotoFragment"
        android:label="Photo"
        tools:layout="@layout/fragment_photo" />
    <fragment
        android:id="@+id/pagerPhotoFragment"
        android:name="com.liyaan.calculationtest.gallery.fragment.PagerPhotoFragment"
        android:label="fragment_pager_photo"
        tools:layout="@layout/fragment_pager_photo" />
</navigation>